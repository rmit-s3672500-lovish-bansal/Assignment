
      
 <div class="container">
    <% flash.each do |key, value| %>
      <%= content_tag(:div, class: "alert alert-#{key} alert-dismissable") do %>
        <%= value %>
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
    <% end %>
  <% end %>
  <!-- Nav tabs -->
  <ul class="nav nav-tabs" role="tablist">
<li role="presentation"><%= link_to "Course", courses_path%></li>
    <li role="presentation" class="active"><%= link_to "Coordinators", coordinator_path, remote: true %></li>
  </ul>
  <!-- Tab panes -->
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane active" id="courses">
        <% User.all.each do |coord| %>
        <div class="row">
          <div class="col-md-3 col-md-offset-2">
            <div class="thumbnail">
              <%= link_to gravatar_for(coord, size: 150), coord %>
              <div class="caption">
                <h4><%= coord.name %></h4>
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>